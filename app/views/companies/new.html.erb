<h1>New Company</h1>

<!--<%= render 'form' %>-->

<%= form_for :company, url: companies_path do |f| %>
  <p>
    <%= f.label :name, "Name" %><br>
    <%= f.text_field :name %>
  </p>
 
  <p>
    <%= f.label :president, "President" %><br>
    <%= f.text_field :president %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>

<%= link_to 'Back', companies_path %>
